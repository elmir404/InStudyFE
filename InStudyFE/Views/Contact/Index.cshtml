	<script>


	    const $lang = localStorage.getItem('lang');
		
		$.ajax({
			type: 'GET',
			headers: {
				'Content-Type': 'application/json',
				'Access-Control-Allow-Origin': '*',

			},
			url: `https://api.instudy.net/api/Program/GetPrograms`,

			success: function (data) {
				var html = [];
				$.each(
					data.data, function (i, value) {
						   if ($lang == 'AZ') {
                            var name = value?.azName

                        }
                        else if ($lang == 'EN') {
                            var name = value?.enName

                        } else {
                            var name = value?.ruName;

                        }
						html.push(
							`
																	<option value="${value.id}">${name}</option>

																 `
						)

					}
				)
				$('#program').html(html.join('')).multipleSelect();
			}

		});
		$.ajax({
			type: 'GET',
			headers: {
				'Content-Type': 'application/json',
				'Access-Control-Allow-Origin': '*',

			},
			url: `https://api.instudy.net/api/Country/GetCountriesIdName`,

			success: function (data) {
				var html = [];
				$.each(
					data.data, function (i, value) {
						   if ($lang == 'AZ') {
                            var name = value?.azName

                        }
                        else if ($lang == 'EN') {
                            var name = value?.enName

                        } else {
                            var name = value?.ruName;

                        }
						html.push(
							`
																			<option value="${value.id}">${name}</option>

																		 `
						);

						$('#requestStudentCountry').append(`<option value="${value.id}">${value.enName}</option>`);
					}
				)
				$('#requestCountry').html(html.join('')).multipleSelect();
			}

		});

		$.ajax({
			type: 'GET',
			headers: {
				'Content-Type': 'application/json',
				'Access-Control-Allow-Origin': '*',

			},
			url: `https://api.instudy.net/api/Direction/GetActiveDirections`,

			success: function (data) {
				var html = [];
				$.each(
					data.data, function (i, value) {
						   if ($lang == 'AZ') {
                            var name = value?.azName

                        }
                        else if ($lang == 'EN') {
                            var name = value?.enName

                        } else {
                            var name = value?.ruName;

                        }
						html.push(
							`
																	<option value="${value.id}">${name}</option>

																 `
						)

					}
				)
				$('#requestDirection').html(html.join('')).multipleSelect({position: 'top'});
			}

		});
	</script>
