											<div class="col-md-6 mb-2">
												<div class="form-group">
												<label id="labelname"for="phone">Name</label>
													<input type="text" class="form-control" name="name" id="requestName" placeholder="Name" required>
													<div id="nameValidation" class="invalid-feedback">
														Please type name.
													</div>
												</div>
											</div>
										<div class="col-md-6 mb-2">
												<div class="form-group">
												<label id="labellast" for="phone">Surname</label>
													<input type="text" class="form-control" name="lastname" id="lastName" placeholder="Lastname" required>
													<div id="lastNameValidation" class="invalid-feedback">
        Please type a last name.
      </div>
												</div>
											</div>
										<div class="col-md-6 mb-2">
											<div class="form-group">
												<label id="labelEmail" for="phone">Email</label>
													<input type="email" class="form-control" name="email" id="requestEmail" placeholder="Email" required>
													<div id="emailNameValidation" class="invalid-feedback">
														Please choose a email.
													</div>
												</div>
											</div>
							
										
												<div class="col-md-6 mb-2">
												<label id="labelphone" for="phone">Phone</label>
												<input type="tel" class="form-control" id="phone" placeholder="Phone number" required>
												<div id="phnoneValidation" class="invalid-feedback">
        Please choose a phone number.
      </div>

											</div>
											
										
								
										<div class="form-group col-md-6" id="">
											<label id="labeldescription" for="comment">Description:</label>
												<textarea class="form-control overflow-auto" name="name" rows="3" id="requestMessage" required></textarea  >
												<div id="messageValidation" class="invalid-feedback">
													Please type a message.
												</div>
										</div>
