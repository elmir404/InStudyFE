                                    {
                                        <h1>@Model.country.EnName</h1> 
                                    }
                                    else if (culture == "ru")
                                    {

                                         <h1>@Model.country.RuName</h1> 
                                    }
                                    else
                                    {
                                         <h1>@Model.country.AzName</h1> 
                                    }
                                </section>
                            </div>
                        </div>
                    </section>
                </div>

            </div>
        </div>
    </section>
