                                        <ul id="directionCon" class="AutoComplete" style="z-index:101;">
                                        </ul>
                                    </div>
                                </div>
                                <button type="button" id="searchButtonCon" style="height:2.5rem;" class="SearchButton"> <i class="lnr-magnifier SearchButtonIcon"></i> </button>
