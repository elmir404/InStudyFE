                            <div class="check-div d-flex justify-content-between align-items-center">
                                <div class="toggle-switch d-flex align-items-center">
                                    
                                </div>
                            </div>
                            <div class="text-center"> <button type="submit" id="loginBtnm" class="btn-blue w-100">Daxil Ol</button> </div>
                        </form>
                    </div>
                </div>
            </div>
           
        </div>
